{
  "name": "deep-require-cwd",
  "version": "1.0.0",
  "description": "Require a module like `require()` but via a sequence of nested packages from the current working directory",
  "main": "index.js",
  "typings": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "test": "tape test.js && mos t && standard",
    "md": "mos"
  },
  "mos": {
    "plugins": [
      "readme"
    ],
    "installation": {
      "useShortAlias": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zkochan/deep-require-cwd.git"
  },
  "keywords": [
    "require",
    "resolve",
    "path",
    "module",
    "from",
    "like",
    "import"
  ],
  "author": {
    "name": "Zoltan Kochan",
    "url": "https://www.kochan.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zkochan/deep-require-cwd/issues"
  },
  "homepage": "https://github.com/zkochan/deep-require-cwd#readme",
  "devDependencies": {
    "mos": "^2.0.0-alpha.3",
    "mos-plugin-readme": "^1.0.4",
    "standard": "^10.0.2",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "deep-require-from": "^1.0.0"
  }
}
